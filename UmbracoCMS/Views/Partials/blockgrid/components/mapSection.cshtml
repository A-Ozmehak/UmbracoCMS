@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<Umbraco.Cms.Core.Models.Blocks.BlockGridItem>;
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
    var sectionTitle = Model?.Content.Value<string>("sectionTitle");
    var headline = Model?.Content.Value<string>("headline");
    var subtext = Model?.Content.Value<string>("subtext");
    var buttonLabel = Model?.Content.Value<string>("buttonLabel");
}
<div class="map-container">
    <div class="map">
        <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2035.1615241898567!2d18.051319778108866!3d59.33025707461478!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x465f9d61b5b2a595%3A0xd9fa560691e12d7!2sKlarabergsviadukten%2090%2C%20111%2064%20Stockholm!5e0!3m2!1ssv!2sse!4v1725348598911!5m2!1ssv!2sse" width="600" height="450" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>                
    <div class="overlay"></div>

    <div class="container">
        <div class="text-content">
            <span>@sectionTitle</span>
            <h2>@headline</h2>
            <p>@subtext</p>
        </div>
                    
        <form>
            <div class="input-container">
                <input type="text" placeholder="Name">
                <input type="email" placeholder="Phone">
            </div>
            <div class="input-container">
                <input type="text" placeholder="Email address">
                <select>
                    <option value="0">Select a service</option>
                    <option value="1">Service 1</option>
                    <option value="2">Service 2</option>
                    <option value="3">Service 3</option>
                </select>
            </div>
            <button class="btn-primary">@buttonLabel</button>
        </form>
    </div>
</div>